{
	"name": "mrtalk",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"format": "biome format ./src ./companion ./lib",
		"format:fix": "biome format --write ./src ./companion ./lib",
		"lint": "biome lint ./src ./companion ./lib",
		"lint:fix": "biome lint --write ./src ./companion ./lib",
		"check": "biome check ./src ./companion ./lib",
		"check:fix": "biome check --write ./src ./companion ./lib",
		"companion": "npx tsx companion/index.ts",
		"firehose": "npx tsx companion/firehose.ts",
		"voicevox": "docker run --rm --gpus all -p '127.0.0.1:50021:50021' voicevox/voicevox_engine:nvidia-latest"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^2.0.27",
