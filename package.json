		"@pixiv/three-vrm": "^3.4.2",
		"@recast-navigation/three": "^0.43.0",
		"@ricky0123/vad-web": "^0.0.29",
		"iwer": "^2.1.1",
		"recast-navigation": "^0.43.0",
		"three": "npm:super-three@0.177.0",
		"voicevox-client": "^0.3.1",
		"zod": "3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
